<keymap version="1" name="Emacs copy" parent="Emacs">
  <action id="$Copy">
    <keyboard-shortcut first-keystroke="alt c" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="w" />
    <keyboard-shortcut first-keystroke="meta c" />
  </action>
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="alt p" />
    <keyboard-shortcut first-keystroke="ctrl y" />
    <keyboard-shortcut first-keystroke="meta v" />
  </action>
  <action id="Editor Copy">
    <keyboard-shortcut first-keystroke="meta c" />
  </action>
  <action id="Editor Paste">
    <keyboard-shortcut first-keystroke="meta v" />
  </action>
  <action id="EditorDecreaseFontSize">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="shift minus" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="minus" />
  </action>
  <action id="EditorEscape">
    <keyboard-shortcut first-keystroke="escape" second-keystroke="escape" />
  </action>
  <action id="EditorIncreaseFontSize">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="shift equals" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="shift add" />
  </action>
  <action id="EditorPreviousWord">
    <keyboard-shortcut first-keystroke="ctrl left" />
    <keyboard-shortcut first-keystroke="alt b" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="ctrl b" />
    <keyboard-shortcut first-keystroke="ctrl l" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl alt g" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="period" />
    <keyboard-shortcut first-keystroke="alt period" />
    <mouse-shortcut keystroke="control button1" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl g" />
  </action>
  <action id="Macro.save_and_optimize">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="ctrl s" />
  </action>
  <action id="MoveEditorToOppositeTabGroup">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl o" />
  </action>
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl m" />
  </action>
  <action id="ParameterInfo">
    <keyboard-shortcut first-keystroke="shift alt p" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl p" />
  </action>
  <action id="SaveAll">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl s" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="ctrl alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="ctrl alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="ctrl alt up" />
  </action>
  <action id="ZoomCurrentWindow">
    <keyboard-shortcut first-keystroke="shift ctrl back_slash" />
  </action>
</keymap>